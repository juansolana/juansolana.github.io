---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Projects</title>
	<link href="http://juansolana.github.io/projects/atom.xml" rel="self"/>
	<link href="http://juansolana.github.io/projects"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://juansolana.github.io/projects</id>
	<author>
		<name>Juan Solana</name>
		<email>jsolanacasas@gmail.com</email>
	</author>

	{% for project_post in site.projposts %}
		<entry>
			<title>{{ project_post.title }}</title>
			<link href="http://juansolana.github.io{{ project_post.url }}"/>
			<updated>{{ project_post.date | date_to_xmlschema }}</updated>
			<id>http://juansolana.github.io{{ project_post.id }}</id>
			<content type="html">{{ project_post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
