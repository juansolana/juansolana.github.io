---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Juan Solana's Projects</title>
	<link href="http://juansolana.github.io/blog/atom.xml" rel="self"/>
	<link href="http://juansolana.github.io/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://juansolana.github.io/blog</id>
	<author>
		<name>Juan Solana</name>
		<email>jsolanacasas@gmail.com</email>
	</author>

	{% for project in site.projects_posts %}
		<entry>
			<title>{{ project.title }}</title>
			<link href="http://juansolana.github.io{{ project.url }}"/>
			<updated>{{ project.date | date_to_xmlschema }}</updated>
			<id>http://juansolana.github.io{{ project.id }}</id>
			<content type="html">{{ project.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
